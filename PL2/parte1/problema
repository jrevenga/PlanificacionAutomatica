( defproblem problem dron

    (
        (DRON dron1)
        (CAJA c1) (CAJA c2) (CAJA c3) (CAJA c4) (CAJA c5) (CAJA c6)
        (LOCATION deposito) (LOCATION l1) (LOCATION l2) (LOCATION l3) (LOCATION l4)
        (PERSONA p1) (PERSONA p2) (PERSONA p3) (PERSONA p4) (PERSONA p5)
        (CONTENIDO comida) (CONTENIDO medicina)
		(BRAZO brazo1) (BRAZO brazo2)
        (esta-dron dron1 deposito)
        ;(dron-libre dron1)
		(libre brazo1)
		(libre brazo2)
        (esta-caja c1 deposito)
        (esta-caja c2 deposito)
        (esta-caja c3 deposito)
        (esta-caja c4 deposito)
        (esta-caja c5 deposito)
        (esta-caja c6 deposito)
        (almacena c1 medicina)
        (almacena c2 comida)
        (almacena c3 medicina)
        (almacena c4 comida)
        (almacena c5 comida)
        (almacena c6 medicina)
        (esta-persona p1 l1)
        (esta-persona p2 l1)
        (esta-persona p3 l2)
        (esta-persona p4 l3)
        (esta-persona p5 l4)
        (necesita p1 medicina)
        (necesita p1 comida)
        (necesita p2 comida)
        (necesita p3 comida)
        (necesita p4 medicina)
        (necesita p5 medicina)
    )

    ( ; TASKS
        (enviar-todo)
    )
)